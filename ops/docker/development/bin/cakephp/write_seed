#!/bin/sh

# ---------------------------
#
# docker-compose alias
#
# ---------------------------

cd `dirname $0`/../../../../../

clear >/dev/tty
echo "Give Me Target Table Name (e.g. Articles): " >/dev/tty
# Show cursor
tput cnorm >/dev/tty 2>/dev/null || tput vs >/dev/tty 2>/dev/null
read input </dev/tty
ops/docker/development/bin/cakephp/cake "bake seed --data $input"
